{
	"name": "ronalds-website",
	"private": true,
	"version": "4.1.0",
	"description": "My personal website. Powered using React, Vite and Bulma.",
	"author": {
	  "name": "Ron Eliass Turner",
	  "email": "roneturner23@gmail.com"
	},
	"type": "module",
	"scripts": {
	  "dev": "vite",
	  "dev:css": "tailwindcss -i ./src/styles/index.css -o ./dist/output.css --watch",
	  "build": "tsc && vite build",
	  "deploy": "gh-pages -d dist",
	  "format": "prettier --write src",
	  "format:check": "prettier --check src",
	  "lint": "eslint src --ext ts,tsx --max-warnings 0",
	  "lint:fix": "eslint src --ext ts,tsx --fix",
	  "preview": "vite preview"
	},
	"dependencies": {
	  "react": "^18.0.0-beta-5",
	  "react-dom": "^18.0.0-beta-5",
	  "react-router-dom": "^6.2.1"
	},
	"devDependencies": {
	  "@types/react": "^18.0.0-beta-5",
	  "@types/react-dom": "^18.0.0-beta-5",
	  "@typescript-eslint/eslint-plugin": "^5.0.0",
	  "@typescript-eslint/parser": "^5.0.0",
	  "@vitejs/plugin-react-swc": "^3.5.0",
	  "autoprefixer": "^10.4.16",
	  "eslint": "^8.5.0",
	  "eslint-plugin-prettier": "^5.0.1",
	  "eslint-plugin-react-hooks": "^4.6.0",
	  "eslint-plugin-react-refresh": "^0.4.5",
	  "gh-pages": "^3.2.3",
	  "postcss": "^8.4.33",
	  "prettier": "^3.1.1",
	  "prettier-plugin-tailwindcss": "^0.5.11",
	  "sass": "^1.49.7",
	  "tailwindcss": "^3.0.0",
	  "typescript": "^4.6.2",
	  "vite": "^3.0.5"
	}
  }
  